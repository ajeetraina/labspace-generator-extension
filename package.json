{
  "name": "github-labspace-generator",
  "version": "1.0.0",
  "description": "Docker Desktop Extension for generating labspaces from GitHub repositories",
  "main": "src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon src/server.js",
    "build": "vite build",
    "build:extension": "docker build -t labspace-generator:latest .",
    "install:extension": "docker extension install labspace-generator:latest",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "archiver": "^6.0.1",
    "yaml": "^2.3.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.292.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajeetraina/labspace-generator-extension.git"
  },
  "keywords": [
    "docker",
    "labspace",
    "github",
    "development-environment",
    "devcontainer",
    "docker-compose"
  ],
  "author": "Ajeet Singh Raina",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["@testing-library/jest-dom"]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}